  "version": "1.0.0",
  "type": "commonjs",
  "scripts": {
    "start": "cross-env PORT=3000 node backend/server.js",
    "dev": "cross-env PORT=3001 concurrently \"node backend/server.js\" \"npm run watch:css\"",
    "build:css": "tailwindcss -i ./backend/public/css/main.css -o ./backend/public/css/style.css --minify",
    "watch:css": "tailwindcss -i ./backend/public/css/main.css -o ./backend/public/css/style.css --watch"
  },
  "dependencies": {
    "bcrypt": "^5.1.0",
    "body-parser": "^1.20.2",
    "cors": "^2.8.5",
    "dotenv": "^17.2.3",
    "express": "^4.18.2",
    "jsonwebtoken": "^9.0.2",
    "mssql": "^9.1.1",
    "qrcode": "^1.5.4"
  },
  "devDependencies": {
    "@tailwindcss/aspect-ratio": "^0.4.2",
    "@tailwindcss/forms": "^0.5.10",
    "@tailwindcss/typography": "^0.5.19",
    "autoprefixer": "^10.4.21",
    "concurrently": "^9.2.1",
    "cross-env": "^7.0.3",
    "postcss": "^8.5.6",
    "postcss-cli": "^11.0.1",
    "tailwindcss": "^3.4.1"
  }
